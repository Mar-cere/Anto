    "test:unit": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=unit",
    "test:integration": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration"
  },
  "keywords": [
    "backend",
    "api",
    "express",
    "mongodb",
    "openai",
    "socket.io",
    "terapia",
    "ai"
